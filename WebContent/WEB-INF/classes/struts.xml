				</interceptor-ref>
				<interceptor-ref name="checkbox" />
				<interceptor-ref name="staticParams" />
				<interceptor-ref name="actionMappingParams" />
				<interceptor-ref name="params">
					<param name="excludeParams">dojo\..*,^struts\..*</param>
				</interceptor-ref>
				<interceptor-ref name="conversionError" />
				<interceptor-ref name="validation">
					<param name="excludeMethods">input,back,cancel,browse</param>
				</interceptor-ref>
				<interceptor-ref name="workflow">
					<param name="excludeMethods">input,back,cancel,browse</param>
				</interceptor-ref>
		   </interceptor-stack>
		   <interceptor-stack name="SessionCheckStack">
				<interceptor-ref name="SessionIntercept" />
				<interceptor-ref name="sessionExpiryStack" />
		   </interceptor-stack>
      		</interceptors>
      		<default-interceptor-ref name="SessionCheckStack"></default-interceptor-ref>
      		<global-results>
				<result name="invalid.token" type="tiles">SessionExpired</result>
      			<result name="session" type="tiles">SessionExpired</result>
      			<result name="SessionExpired" type="tiles">SessionExpired</result>
      			<result name="unhandledException" type="tiles">SessionExpired</result>
      			<result name="ExceptionHandler" type="chain">
		        <param name="actionName">ExceptionHandler</param>
		    </result>
      		</global-results>
      		<global-exception-mappings>
      			<exception-mapping result="ExceptionHandler" exception="java.lang.Exception" />
      			<exception-mapping exception="java.lang.Exception" result="ExceptionHandler" />
			    <exception-mapping exception="java.lang.Error" result="ExceptionHandler" />
			    <exception-mapping exception="java.lang.RuntimeException" result="ExceptionHandler" />
      		</global-exception-mappings>
